[
  ~[tlist_sql;
    SELECT
      id,
      CASE
      WHEN contactdcid IS NULL
        THEN 'null'
      ELSE '"' || contactdcid || '"' END,
      CASE
      WHEN phone_number IS NULL
        THEN 'null'
      ELSE '"' || phone_number || '"' END,
      CASE
      WHEN phone_type IS NULL
        THEN 'null'
      ELSE '"' || phone_type || '"' END,
      CASE WHEN row_number()
                        OVER (
                          ORDER BY NULL) = count(*)
                        OVER ()
                THEN ''
              ELSE ',' END AS comma
    FROM
      u_sc_phone
    WHERE
      studentsdcid = ~(gpv.students_dcid)]
      {
        "phone_id": ~(phone_id),
        "contactdcid": ~(contactdcid),
        "phone_number": ~(phone_number),
        "phone_type": ~(phone_type)
      }~(comma)
  [/tlist_sql]
]
